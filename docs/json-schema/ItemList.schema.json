{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://vizztools.github.io/vizzToolsCore/json-schema/ItemList.schema.json",
    "type": "object",
    "title": "ItemList",
    "description": "A list of items of any sortâ€”for example, Top 10 Movies About Weathermen, or Top 100 Party Songs. Not to be confused with HTML lists, which are often used only for formatting.",
    "required": ["@context", "type", "itemListElement"],
    "properties": {
        "@context": {
            "$ref": "@context.schema.json"
        },
        "@type": {
            "$ref": "@type.schema.json"
        },
        "itemListElement": {
            "type": "array",
            "title": "itemListElement",
            "items": {
                "anyOf": [
                    {
                        "type": "string"
                    }
                ]
            }
        },
        "itemListOrder": {
            "type": "string",
            "title": "itemListOrder",
            "description": "Type of ordering (e.g. Ascending, Descending, Unordered)."
        },
        "name": {
            "type": "string",
            "description": "Name of the ItemList."
        }
    }
}